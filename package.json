{
  "name": "@centy-io/assets",
  "version": "0.0.0",
  "description": "Centy brand assets - logos, icons, and design resources",
  "type": "module",
  "exports": {
    "./logos/*": "./logos/*"
  },
  "files": [
    "logos"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{json,md}\"",
    "format:check": "prettier --check \"**/*.{json,md}\"",
    "spell": "cspell lint '**/*.{md,json}' --gitignore",
    "prepare": "husky",
    "release": "release-it"
  },
  "keywords": [
    "centy",
    "assets",
    "logos",
    "brand",
    "design",
    "svg"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.24.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@cspell/dict-he": "^4.0.5",
    "@eslint/js": "^9.39.1",
    "cspell": "^9.4.0",
    "eslint": "^9.39.1",
    "eslint-config-publishable-package-json": "^1.0.0",
    "husky": "^9.1.7",
    "jsonc-eslint-parser": "^2.4.1",
    "prettier": "^3.7.4",
    "release-it": "^19.0.6"
  }
}
